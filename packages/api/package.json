{
  "name": "api",
  "version": "0.0.1-alpha",
  "description": "API and static webserver for Artistware core",
  "main": "./build/index.js",
  "engines": {
    "node": "10.15.3",
    "yarn": "1.16.0"
  },
  "scripts": {
    "start": "set DEBUG=* && ts-node-dev --transpileOnly --respawn ./src/index.ts",
    "build": "./../../node_modules/.bin/tsc && yarn run build:move",
    "build:move": "node ./buildScripts/buildCopy.js",
    "build:clean": "rimraf build && yarn run build"
  },
  "author": "Brandon Morrison <brandon@brandonam.com> (http://brandonam.com)",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "express": "^4.17.0",
    "express-graphql": "^0.8.0",
    "graphql": "^14.3.0",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "sequelize": "^5.8.6",
    "ts-class-validator": "^0.2.2",
    "typeorm": "^0.2.17",
    "typescript": "^3.4.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.8.0",
    "@types/graphql": "^14.2.0",
    "@types/helmet": "^0.0.43",
    "@types/jsonwebtoken": "^8.3.2",
    "graphiql": "^0.13.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "tslint": "^5.16.0"
  }
}
